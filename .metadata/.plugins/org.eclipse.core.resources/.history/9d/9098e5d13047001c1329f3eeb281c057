package br.com.alura;

public class Aula {

	String titulo;
	int tempo;
	
	public String getTitulo() {
		return titulo;
	}
	
	public int getTempo() {
		return tempo;
	}
	
	public Aula(String titulo, int tempo) {
		this.titulo = titulo;
		this.tempo = tempo;
	}
	
	public String toString() {
		return "Aula: " + this.titulo + "Duração: " + this.tempo + " minutos";
	}
}